            android:theme="@style/AppTheme.Dialog.NoScrim"/>
        <activity
            android:name=".apply.database.smartisan.SmartisanApplyProgressActivity"
            android:theme="@style/AppTheme.Dialog.NoScrim"/>
        <activity
            android:name=".apply.database.googleNow.GoogleNowApplyProgressActivity"
            android:theme="@style/AppTheme.Dialog.NoScrim"/>
        <activity
            android:name=".apply.database.pixel.PixelApplySingleInstanceProgressActivity"
            android:launchMode="singleInstance"
            android:theme="@style/AppTheme.Dialog.NoScrim"/>
        <activity
            android:name=".test.TestActivity"
            android:theme="@style/AppTheme.Transparent"/>

        <receiver android:name=".receivers.PackageReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_INSTALL"/>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <action android:name="android.intent.action.PACKAGE_REMOVED"/>
                <action android:name="android.intent.action.PACKAGE_REPLACED"/>

                <data android:scheme="package"/>
            </intent-filter>
        </receiver>

    </application>

</manifest>