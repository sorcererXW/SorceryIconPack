            <!-- <action android:name="android.intent.action.GET_CONTENT"/> -->


            <!-- <category android:name="android.intent.category.OPENABLE"/> -->
            <!-- <category android:name="android.intent.category.DEFAULT"/> -->


            <!-- <data android:mimeType="image/*"/> -->
            <!-- </intent-filter> -->

            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts"/>
        </activity>
        <activity
            android:name=".ApplyActivity"
            android:label="@string/nav_item_apply"
            android:theme="@style/SubActivity"/>
        <activity
            android:name=".DonateActivity"
            android:label="@string/nav_item_donate"
            android:theme="@style/SubActivity"/>
        <activity
            android:name=".IconRequestActivity"
            android:exported="true"
            android:label="@string/request"
            android:theme="@style/SubActivity"/>
        <activity
            android:name=".HelpActivity"
            android:label="@string/nav_item_help"
            android:theme="@style/SubActivity">
        </activity>
        <activity
            android:name=".showcase.detail.IconDialogActivity"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.Dialog"/>

        <meta-data
            android:name="xposedmodule"
            android:value="true"/>
        <meta-data
            android:name="xposeddescription"
            android:value="全局加载Sorcery图标, 勾选并去'图标包->应用图标->xposed'里面打开\nTo use Sorcery icons systemically, check and apply in 'iconpack->apply->xposed'"/>
        <meta-data
            android:name="xposedminversion"
            android:value="30"/>

        <activity
            android:name=".SearchActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/SearchActivity"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:exported="true"
            android:label="@string/nav_item_settings"
            android:theme="@style/SubActivity"/>
        <activity
